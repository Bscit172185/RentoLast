<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RENTO" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowEnterTransition">@transition/trans_anim</item>
        <item name="android:windowExitTransition">@transition/trans_anim</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="round" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerRadius">150dp</item>
        <item name="cornerSize">75dp</item>
        <item name="color">@color/white</item>

    </style>
    <style name="circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerRadius">25dp</item>
        <item name="cornerSize">25dp</item>
        <item name="color">@color/white</item>
    </style>

</resources>